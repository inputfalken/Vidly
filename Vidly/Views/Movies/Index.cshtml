@model IEnumerable<Vidly.Models.Movie>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Movies</h1>
<div class="panel panel-default">
    <div class="panel-heading">Movie</div>
    <ul class="list-group">
        @foreach (var item in Model) {
            <li class="list-group-item"> @item.Name </li>
        }

    </ul>
</div>